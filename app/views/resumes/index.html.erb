<div class="container">
<p id="notice"><%= notice %></p>

<h1>Listing Resumes</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Location</th>
      <th>Text</th>
      <th>Artists</th>
      <th>Curator</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @resumes.each do |resume| %>
      <tr>
        <td><%= resume.title %></td>
        <td><%= resume.year %></td>
        <td><%= resume.location %></td>
        <td><%= resume.text %></td>
        <td><%= resume.artists %></td>
        <td><%= resume.curator %></td>
        <td><%= resume.link %></td>
        <td><%= link_to 'Show', resume %></td>
        <td><%= link_to 'Edit', edit_resume_path(resume) %></td>
        <td><%= link_to 'Destroy', resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Resume', new_resume_path %>

</div>