<%= simple_form_for(@resume) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :year, collection: 2007..Date.today.year, prompt: "Select year" %>
    <%= f.input :location %>
    <%= f.input :text %>
    <%= f.input :artists %>
    <%= f.input :curator %>
    <%= f.input :link %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
